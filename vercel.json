        "version": 2,
        "builds": [
            {
                "src": "./index.js",
                "use": "@vercel/node"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "/"
            }
        ],
    "env": {
        "url": "@url"
    },
    "build": {
        "env": {
            "url": "@url"
        }
    }
}